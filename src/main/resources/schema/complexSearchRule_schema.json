{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "ComplexSearchRule",
  "description": "validate complexSearchRule",
  "type": "object",
  "properties": {
    "type": {
      "description": "Type of the SearchRule",
      "type": "string",
      "enum": [
        "table",
        "combobox",
        "dropdown",
        "droplist",
        "menu"
      ]
    },
    "innerSearchRules": {
      "description": "List of inner SearchRules",
      "type": "array",
      "items": {
        "oneOf": [
          {
            "type": "object",
            "properties": {
              "uniqueness": {
                "description": "Uniqueness element",
                "type": "string"
              },
              "title": {
                "description": "Name of field name into complex annotations",
                "type": "string",
                "enum": [
                  "root",
                  "jroot",
                  "jRoot",
                  "level1"
                ]
              },
              "selector": {
                "type": "object",
                "properties": {
                  "type": {
                    "description": "Type of the selector",
                    "type": "string",
                    "enum": [
                      "xpath",
                      "css"
                    ]
                  },
                  "value": {
                    "description": "Value of the selector",
                    "type": "string"
                  }
                },
                "required": [
                  "type",
                  "value"
                ]
              }
            },
            "required": [
              "title",
              "uniqueness",
              "selector"
            ]
          },
          {
            "type": "object",
            "properties": {
              "uniqueness": {
                "description": "Uniqueness element",
                "type": "null"
              },
              "title": {
                "description": "Name of field name into complex annotations",
                "type": "string",
                "enum": [
                  "list",
                  "jlist",
                  "value",
                  "jvalue",
                  "expand",
                  "jexpand",
                  "level2",
                  "level3",
                  "level4",
                  "level5",
                  "input",
                  "jInput",
                  "searchButton",
                  "jSearchButton",
                  "jSuggestions",
                  "header",
                  "rowsHeader",
                  "headers",
                  "rowNames",
                  "cell",
                  "row",
                  "column",
                  "footer",
                  "jHeaders",
                  "jRowNames",
                  "jCell",
                  "jRow",
                  "jColumn",
                  "jFooter",
                  "height",
                  "width",
                  "size",
                  "rowStartIndex",
                  "colStartIndex",
                  "headerType",
                  "useCache"
                ]
              },
              "selector": {
                "type": "object",
                "properties": {
                  "type": {
                    "description": "Type of the selector",
                    "type": "string",
                    "enum": [
                      "xpath",
                      "css"
                    ]
                  },
                  "value": {
                    "description": "Value of the selector",
                    "type": "string"
                  }
                },
                "required": [
                  "type",
                  "value"
                ]
              }
            },
            "required": [
              "title",
              "selector"
            ]
          }
        ]
      }
    }
  },
  "required": [
    "type",
    "innerSearchRules"
  ]
}